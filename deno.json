{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "indexing": "deno run -A indexing.ts",
    "esm:add": "deno run -A https://esm.sh/v125 add",
    "esm:update": "deno run -A https://esm.sh/v125 update",
    "esm:remove": "deno run -A https://esm.sh/v125 remove"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "lock": false,
  "imports": {
    "@/": "./",
    "$std/": "https://deno.land/std@0.191.0/",
    "$mock_file/": "https://deno.land/x/mock_file@v1.1.2/",
    "$og_edge/": "https://deno.land/x/og_edge@0.0.6/",
    "$sqlite/": "https://deno.land/x/sqlite@v3.7.2/",
    "@orama/orama": "https://esm.sh/v125/@orama/orama@1.0.6",
    "@orama/orama/": "https://esm.sh/v125/@orama/orama@1.0.6/",
    "@orama/plugin-data-persistence": "https://esm.sh/v125/*@orama/plugin-data-persistence@1.0.6",
    "@orama/plugin-data-persistence/": "https://esm.sh/v125/*@orama/plugin-data-persistence@1.0.6/",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "dpack": "https://esm.sh/v125/dpack@0.6.22",
    "feed": "https://esm.sh/v125/*feed@4.2.2",
    "marked": "https://esm.sh/v125/marked@5.0.5",
    "marked/": "https://esm.sh/v125/marked@5.0.5/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.1.0",
    "preact-render-to-string/": "https://esm.sh/v125/*preact-render-to-string@6.1.0/",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "printd": "https://esm.sh/v125/printd@1.6.0",
    "schema-dts": "https://esm.sh/v125/*schema-dts@1.1.2",
    "$fresh/": "https://deno.land/x/fresh@1.2.0/"
  },
  "scopes": {
    "https://deno.land/x/og_edge@0.0.6/": {
      "https://esm.sh/react@18.2.0": "https://esm.sh/preact@10.14.1/compat"
    },
    "https://esm.sh/v125/": {
      "@msgpack/msgpack": "https://esm.sh/v125/@msgpack/msgpack@2.8.0",
      "pretty-format": "https://esm.sh/v125/pretty-format@3.8.0",
      "xml-js": "https://esm.sh/v125/xml-js@1.6.11"
    }
  }
}